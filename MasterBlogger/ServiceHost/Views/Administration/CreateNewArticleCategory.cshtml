@using MB.Application.Contacts.ArticleCategory;
@{
    Layout = "_AdministrationLayout";
}
@model CreateArticleCategory

<h1>Create ArticleCategory</h1>
<form method="post">
    <div class="mb-3">
        <label for="title" class="form-label">Title</label>
        <input asp-for="@Model.Title" type="text" name="title" class="form-control" placeholder="Enter Title">
    </div>
    <button asp-action="FuncCreatedArticleCategory" type="submit" class="btn btn-primary">Submit</button>
</form>
